{
	"version": 8,
	"sources": {
		"mapzen": {
			"type": "vector",
			"tiles": [
				"https://tile.mapzen.com/mapzen/vector/v1/all/{z}/{x}/{y}.mvt?api_key=mapzen-BtoKQDi"
			],
			"maxzoom": 16
		}
	},
	"glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
	"layers": [
		{
			"id": "background",
			"type": "background",
			"paint": {"background-color": "#F5F5F3"}
		},
		{
			"id": "park",
			"type": "fill",
			"source": "mapzen",
			"source-layer": "landuse",
			"filter": [
				"in",
				"kind",
				"park",
				"national_park",
				"nature_reserve",
				"forest",
				"garden",
				"grass",
				"farm",
				"meadow",
				"playground",
				"golf_course",
				"wetland",
				"wood",
				"cemetery"
			],
			"paint": {
				"fill-antialias": false,
				"fill-color": "hsl(0, 0%, 89%)",
				"fill-opacity": 0.1
			},
			"min-zoom": 9
		},
		{
			"id": "water shadow",
			"filter": ["==", "$type", "Polygon"],
			"paint": {
				"fill-color": "hsl(185, 7%, 73%)",
				"fill-opacity": 1,
				"fill-translate": {"base": 1.2, "stops": [[7, [0, 0]], [16, [-1, -1]]]},
				"fill-translate-anchor": "viewport"
			},
			"source": "mapzen",
			"source-layer": "water",
			"type": "fill"
		},
		{
			"id": "water",
			"paint": {"fill-color": "hsl(185, 9%, 81%)"},
			"ref": "water shadow"
		},
		{
			"id": "river",
			"type": "line",
			"source": "mapzen",
			"source-layer": "water",
			"filter": ["all", ["==", "$type", "LineString"], ["==", "kind", "river"]],
			"layout": {
				"line-cap": {"base": 1, "stops": [[0, "butt"], [11, "round"]]},
				"line-join": "round"
			},
			"paint": {
				"line-color": "#cbd3d4",
				"line-opacity": {"base": 1, "stops": [[8, 0], [8.5, 1]]},
				"line-width": {"base": 1.3, "stops": [[8.5, 0.1], [20, 8]]}
			},
			"minzoom": 8
		},
		{
			"id": "country-boundary",
			"type": "line",
			"source": "mapzen",
			"source-layer": "boundaries",
			"filter": [
				"all",
				["==", "kind", "country"],
				["!=", "maritime_boundary", true]
			],
			"layout": {"line-cap": "round", "line-join": "round"},
			"paint": {
				"line-color": "#aaaaaa",
				"line-dasharray": [4, 2, 2, 1, 2],
				"line-width": {"base": 2, "stops": [[1, 0.5], [7, 3]]}
			},
			"max-zoom": 4
		},
		{
			"id": "disputed-boundary",
			"type": "line",
			"source": "mapzen",
			"source-layer": "boundaries",
			"filter": [
				"all",
				[
					"in",
					"kind",
					"disputed",
					"indefinite",
					"indeterminate",
					"lease_limit",
					"line_of_control",
					"overlay_limit"
				]
			],
			"layout": {"line-cap": "round", "line-join": "round"},
			"paint": {
				"line-color": "#aaaaaa",
				"line-dasharray": [4, 2, 2, 1, 2],
				"line-width": {"base": 2, "stops": [[1, 0.5], [7, 3]]}
			},
			"max-zoom": 4
		},
		{
			"id": "state-boundary",
			"type": "line",
			"source": "mapzen",
			"source-layer": "boundaries",
			"filter": [
				"all",
				["in", "kind", "region", "macroregion"],
				["!=", "maritime_boundary", true],
				["==", "min_zoom", "2"]
			],
			"layout": {"line-cap": "round", "line-join": "round"},
			"paint": {
				"line-color": "#aaaaaa",
				"line-dasharray": [4, 2, 2, 1, 2],
				"line-width": {"base": 2, "stops": [[1, 0.25], [7, 1.5]]}
			},
			"max-zoom": 8
		},
		{
			"id": "state-boundary-high-zoom",
			"type": "line",
			"source": "mapzen",
			"source-layer": "boundaries",
			"filter": [
				"all",
				["in", "kind", "region", "macroregion"],
				["!=", "maritime_boundary", true]
			],
			"layout": {"line-cap": "round", "line-join": "round"},
			"paint": {
				"line-color": "#bbbbbb",
				"line-dasharray": [4, 2, 2, 1, 2],
				"line-width": {"base": 2, "stops": [[1, 0.25], [7, 1.5]]}
			},
			"min-zoom": 8,
			"max-zoom": 10
		},
		{
			"id": "buildings",
			"type": "fill",
			"source": "mapzen",
			"source-layer": "buildings",
			"minzoom": 12,
			"paint": {
				"fill-antialias": true,
				"fill-color": "#E0E0DE",
				"fill-opacity": {"base": 1, "stops": [[12, 0], [16, 1]]}
			}
		},
		{
			"filter": [
				"all",
				["==", "kind", "minor_road"]
			],
			"id": "road-street-low",
			"layout": {"line-cap": "round", "line-join": "round"},
			"minzoom": 11,
			"paint": {
				"line-color": "#E2E2E1",
				"line-width": {"base": 1.5, "stops": [[12.5, 0.5], [14, 2], [18, 18]]}
			},
			"source": "mapzen",
			"source-layer": "roads",
			"type": "line"
		},
		{
			"id": "road-street-case",
			"paint": {
				"line-color": "#E2E2E1",
				"line-gap-width": {"base": 1.5, "stops": [[13, 0], [14, 2], [18, 18]]},
				"line-width": {"base": 1.5, "stops": [[12, 1.25], [20, 2]]}
			},
			"ref": "road-street-low"
		},
		{
			"filter": [
				"all",
				["==", "kind", "major_road"]
			],
			"id": "road-main-case",
			"layout": {"line-cap": "round", "line-join": "round"},
			"paint": {
				"line-color": "#E2E2E1",
				"line-gap-width": {
					"base": 1.5,
					"stops": [[8.5, 0.5], [10, 0.75], [18, 26]]
				},
				"line-width": {"base": 1.2, "stops": [[10, 1.25], [18, 2]]}
			},
			"source": "mapzen",
			"source-layer": "roads",
			"type": "line"
		},
		{
			"filter": [
				"all",
				["==", "kind", "highway"]
			],
			"id": "road-motorway-case",
			"layout": {"line-cap": "round", "line-join": "round"},
			"paint": {
				"line-color": "#E2E2E1",
				"line-gap-width": {"base": 1.5, "stops": [[5, 0.75], [18, 32]]},
				"line-opacity": 1,
				"line-width": {"base": 1.5, "stops": [[5, 1.5], [16, 2]]}
			},
			"source": "mapzen",
			"source-layer": "roads",
			"type": "line"
		},
		{
			"id": "road-street",
			"paint": {
				"line-color": "#fff",
				"line-width": {"base": 1.5, "stops": [[12.5, 0.5], [14, 2], [18, 12]]}
			},
			"ref": "road-street-low"
		},
		{
			"id": "road-secondary-tertiary",
			"paint": {
				"line-color": "#fff",
				"line-width": {"base": 1.5, "stops": [[8.5, 0.5], [10, 0.75], [18, 26]]}
			},
			"ref": "road-main-case"
		},
		{
			"id": "road-motorway",
			"paint": {
				"line-color": "#fff",
				"line-opacity": 1,
				"line-width": {"base": 1.5, "stops": [[5, 0.75], [18, 32]]}
			},
			"ref": "road-motorway-case"
		}
	]
}
